/**app.wxss**/

page {
	height: 100%;
	width: 100%;
	position: fixed;
	left: 0;
	top: 0;
	background-color:#ededed;
	font-family: SourceHanSansCN Medium;
}

.content {
	width: 100%;
	height: 100%;
	position: relative;
}

.fm {
	position: absolute;
	left: 0;
	bottom: 0;
	right: 0;
}

.fm_content{
	width:100%;
	height:100%;
	position: relative;
}

::-webkit-scrollbar {
	display: none;
}
